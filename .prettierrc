{
  "plugins": ["prettier-plugin-tailwindcss"],
  "endOfLine": "lf",
  "trailingComma": "es5",
  "arrowParens": "always",
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": ["**/*.css", "**/*.scss", "**/*.html"],
      "options": { "singleQuote": false }
    }
  ]
}
